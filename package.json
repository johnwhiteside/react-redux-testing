{
  "name": "react-redux-testing",
  "version": "1.0.0",
  "description": "A simple todo list application to demonstrate how to unit test React/Redux applications using Jest.",
  "author": "John Whiteside <johnwhitesideii@gmail.com>",
  "license": "MIT",
  "dependencies": {},
  "scripts": {
    "start": "webpack-dev-server --mode development --open",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:updateSnapshot": "jest -u"
  },
  "jest": {
    "setupFiles": [
      "./jestsetup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "devDependencies": {
    "axios": "^0.18.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^22.4.3",
    "jest-cli": "^23.1.0",
    "lodash": "^4.17.10",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "react-test-renderer": "^16.3.2",
    "redux": "^4.0.0",
    "redux-mock-store": "^1.5.1",
    "redux-thunk": "^2.3.0",
    "reselect": "^3.0.1",
    "rosie": "^2.0.1",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.3",
    "webpack-dev-server": "^3.1.4"
  }
}
